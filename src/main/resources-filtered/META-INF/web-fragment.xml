<web-fragment xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/web-fragment_3_0.xsd"
  version="3.0"
  metadata-complete="false">
	<name>@{project.groupId}:@{project.artifactId}</name>
	<!-- <display-name>@{project.name} @{project.version}</display-name> -->
	<description>@{project.description}</description>

	<!-- Error pages -->
	<error-page>
		<error-code>404</error-code>
		<location>/semanticcms-theme-documentation/error-pages/404.inc.jspx</location>
	</error-page>
	<error-page>
		<error-code>500</error-code>
		<location>/semanticcms-theme-documentation/error-pages/500.inc.jspx</location>
	</error-page>
	<error-page>
		<error-code>503</error-code>
		<location>/semanticcms-theme-documentation/error-pages/503.inc.jspx</location>
	</error-page>

</web-fragment>
